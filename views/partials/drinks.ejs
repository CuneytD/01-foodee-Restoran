<div class="fh5co-food-menu to-animate-2">
    <h2 class="fh5co-drinks">Drinks</h2>
    <ul>
        <% list.forEach(function(icecekler) { %>
        <li>
            <div class="fh5co-food-desc">
                <figure>
                    <img src="/images/<%= icecekler.imageUrl %>" class="img-responsive" alt="Free HTML5 Templates by FREEHTML5.co">
                </figure>
                <div>
                    <h3><%= icecekler.name %></h3>
                    <p><%= icecekler.description %></p>
                </div>
            </div>
            <div class="fh5co-food-pricing">
                $<%= icecekler.price %>
            </div>
        </li>
        <% }); %>
    </ul>
</div>